{
    "name": "iamrahul1973/bootstrap-dashboard-skeleton",
    "description": "A starting point for your Laravel Dashboard using Bootstrap",
    "keywords": [
        "iamRahul1973",
        "laravel",
        "bootstrap-dashboard-skeleton"
    ],
    "homepage": "https://github.com/iamrahul1973/bootstrap-dashboard-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "rahul_cordiace",
            "email": "rahul.k@cordiace.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-permission": "^4.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "IamRahul1973\\Skeleton\\": "src",
            "IamRahul1973\\Skeleton\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IamRahul1973\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IamRahul1973\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "IamRahul1973\\Skeleton\\SkeletonFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
